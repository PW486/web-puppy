{
  "manifest_version": 3,
  "name": "Web Puppy",
  "version": "1.0.0",
  "description": "A cute little white puppy is always with you in the bottom right corner!",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
